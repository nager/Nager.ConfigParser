<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <Authors>Tino Hager</Authors>
    <Company>nager.at</Company>
    <AssemblyVersion>1.2.0</AssemblyVersion>
    <PackageProjectUrl>https://github.com/tinohager/Nager.ConfigParser</PackageProjectUrl>
    <RepositoryUrl>https://github.com/tinohager/Nager.ConfigParser</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.2.0</Version>
    <FileVersion>1.2.0</FileVersion>
    <Description>key value key-value keyvalue configuration config parser</Description>
    <PackageTags>configuration parser key value</PackageTags>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
